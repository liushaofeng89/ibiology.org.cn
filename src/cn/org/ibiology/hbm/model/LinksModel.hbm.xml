<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">

	<class name="cn.org.ibiology.hbm.model.LinksModel" table="_ibiology_links">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>

		<property name="siteName" column="SiteName" type="java.lang.String" not-null="true"/>
		<property name="siteDomain" column="SiteDomain" type="java.lang.String" not-null="true"/>
		<property name="data" column="CreateTime" type="java.util.Date" not-null="true"/>
		<property name="effectiveTime" column="EffectiveTime" type="java.lang.Long" not-null="true"/>

	</class>
</hibernate-mapping>