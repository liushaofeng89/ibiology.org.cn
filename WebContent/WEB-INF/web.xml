	</filter>
	<filter>
		<description>
		</description>
		<display-name>RightCheck</display-name>
		<filter-name>RightCheck</filter-name>
		<filter-class>cn.org.ibiology.filter.RightCheckFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>GZipFilter</filter-name>
		<url-pattern>*.js</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>GZipFilter</filter-name>
		<url-pattern>*.css</url-pattern>
