	<display-name>ibiology</display-name>
	<filter>
		<description>
		</description>
		<display-name>EncodingFilter</display-name>
		<filter-name>EncodingFilter</filter-name>
		<filter-class>cn.org.ibiology.filter.EncodingFilter</filter-class>
	</filter>
	<filter>
		<description>
		</description>
		<display-name>GZipFilter</display-name>
