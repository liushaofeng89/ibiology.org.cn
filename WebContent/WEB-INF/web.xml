	</filter-mapping>
	<filter-mapping>
		<filter-name>GZipFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>GZipFilter</filter-name>
		<url-pattern>/</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>RightCheck</filter-name>
		<url-pattern>/manage/*</url-pattern>
	</filter-mapping>



